  "main": "filter-integration.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "if exist dist rmdir /s /q dist",
    "create-dist": "mkdir dist",
    "copy-files": "robocopy . dist *.html *.css *.js /E /NFL /NDL /NJH /NJS /nc /ns /np && robocopy src dist /E /NFL /NDL /NJH /NJS /nc /ns /np",
    "lint": "next lint && eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:ci": "jest --ci",
    "type-check": "tsc --noEmit",
    "deploy:staging": "npm run build && gh-pages -d dist -b staging",
    "deploy:prod": "npm run build && gh-pages -d dist -b main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrJanDuffy/lonemountainvistas.github.io.git"
  },
