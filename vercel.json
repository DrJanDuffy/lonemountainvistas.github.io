{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "CEREBRAS_API_KEY": "@cerebras_api_key",
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": true,
    "silent": true
  }
} 